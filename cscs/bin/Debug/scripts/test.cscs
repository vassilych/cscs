function calculateNormal(n) {
  returnValue = 0;
  x=0.0; y=0.0;
  for (i = 0; i < n; i++) {
      x = Math.Sqrt((32*8*4 - 10*(3+7) + 19*4) * 10) / 5;
      y = Math.Sin(20) + Math.Cos(0.8);
      returnValue += x + y;
